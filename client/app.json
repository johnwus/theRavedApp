{
  "expo": {
    "name": "Raved",
    "slug": "raved-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logos/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.raved.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logos/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raved.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "webpack"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}